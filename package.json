{
  "name": "playwright-testing-framework",
  "version": "1.0.0",
  "description": "Comprehensive cross-browser Playwright test suite",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:ci": "playwright test --config=playwright.ci.config.ts",
    "test:qa": "set TEST_BASE_URL=https://AZVQSMSWEB360.amr.corp.intel.com:4012&& playwright test --config=playwright.ci.config.ts",
    "test:local": "playwright test --headed --config=playwright.config.ts",
    "validate-auth": "node validate-auth.js",
    "install:browsers": "playwright install"
  },
  "keywords": [
    "playwright",
    "testing",
    "e2e"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "directories": {
    "test": "tests"
  }
}
